<AnchorPane prefHeight="158.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
      <AnchorPane prefHeight="156.0" prefWidth="1201.0" style="-fx-background-color: #F4FAFD;">
          <children>
              <Label fx:id="stationAvailableBike" layoutX="561.0" layoutY="40.0" prefHeight="17.0" prefWidth="381.0" text="Available bikes: 38/50">
